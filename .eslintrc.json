{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier","plugin:jest/recommended"],
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "no-console": "error"
    },
    "plugins": ["jest"]
}
